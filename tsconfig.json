{
    "compilerOptions": {
        "target": "es2016",
        "module": "esnext",
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "outDir": "out",
        "sourceMap": true,
        "strict": true,
        "lib": [
            "dom",
            "es2015",
            "es2016"
          ]
    }
}